{
  "name": "<%= name %>",
  "version": "0.1.0",
  "private": true,
  "description": "Always a pleasure scaffolding your awesome static sites.",
  "keywords": [
    "<%= name %>",
    "boilerplate",
    "pages",
    "zce"
  ],
  "homepage": "https://github.com/<%= github %>/<%= name %>#readme",
  "bugs": {
    "url": "https://github.com/<%= github %>/<%= name %>/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= github %>/<%= name %>.git"
  },
  "license": "MIT",
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>",
    "url": "<%= url %>"
  },
  "scripts": {
    "clean": "x-pages clean",
    "lint": "x-pages lint",
    "serve": "x-pages serve",
    "build": "x-pages build",
    "start": "x-pages start",
    "deploy": "x-pages deploy --production"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "zce"
    ]
  },
  "dependencies": {
    "bootstrap": "4.5.2",
    "jquery": "3.5.1",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "x-pages": "1.0.0"
  },
  "engines": {
    "node": ">=6"
  }
}
