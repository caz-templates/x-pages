{
  "name": "<%= name %>",
  "version": "0.1.0",
  "private": true,
  "description": "Always a pleasure scaffolding your awesome static sites.",
  "keywords": [
    "<%= name %>",
    "boilerplate",
    "pages",
    "zce"
  ],
  "repository": "<%= github %>/<%= name %>",
  "license": "MIT",
  "author": "<%= author %> <<%= email %>> (<%= url %>)",
  "scripts": {
    "clean": "x-pages clean",
    "lint": "x-pages lint",
    "serve": "x-pages serve",
    "build": "x-pages build",
    "start": "x-pages start",
    "deploy": "x-pages deploy --production"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "renovate": {
    "extends": [
      "zce"
    ]
  },
  "dependencies": {
    "bootstrap": "5.0.2",
    "jquery": "3.6.0",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "x-pages": "1.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
